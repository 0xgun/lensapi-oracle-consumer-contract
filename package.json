{
  "name": "lensapi-oracle-consumer-contract",
  "version": "0.0.1",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "compile": "hardhat compile",
    "clean": "hardhat clean",
    "test-deploy": "hardhat run --network mumbai ./scripts/mumbai/deploy.ts",
    "main-deploy": "hardhat run --network polygon ./scripts/polygon/deploy.ts",
    "test-set-attestor": "hardhat run --network mumbai ./scripts/mumbai/set-attestor.ts",
    "main-set-attestor": "hardhat run --network polygon ./scripts/polygon/set-attestor.ts",
    "test-push-request": "hardhat run --network mumbai ./scripts/mumbai/push-request.ts",
    "main-push-request": "hardhat run --network polygon ./scripts/polygon/push-request.ts",
    "test-push-malformed-request": "hardhat run --network mumbai ./scripts/mumbai/push-malformed-request.ts",
    "main-push-malformed-request": "hardhat run --network polygon ./scripts/polygon/push-malformed-request.ts",
    "test-verify": "hardhat verify --network mumbai --constructor-args mumbai.arguments.ts",
    "main-verify": "hardhat verify --network polygon --constructor-args polygon.arguments.ts",
    "build-function": "phat-fn build src/index.ts",
    "run-function": "phat-fn run dist/index.js",
    "localhost-node": "hardhat node",
    "localhost-test": "hardhat test",
    "localhost-deploy": "hardhat run --network localhost ./scripts/localhost/deploy.ts",
    "localhost-watch": "phat-fn watch",
    "localhost-push-request": "hardhat run --network localhost ./scripts/localhost/push-request.ts",
    "test-deploy-function": "hardhat run --network mumbai ./scripts/mumbai/deploy-function.ts",
    "main-deploy-function": "hardhat run --network polygon ./scripts/polygon/deploy-function.ts",
    "test-update-function": "hardhat run --network mumbai ./scripts/mumbai/update-function.ts",
    "main-update-function": "hardhat run --network polygon ./scripts/polygon/update-function.ts"
  },
  "devDependencies": {
    "@ethersproject/abi": "^5.7.0",
    "@ethersproject/providers": "^5.7.2",
    "@nomicfoundation/hardhat-chai-matchers": "^2.0.2",
    "@nomicfoundation/hardhat-ethers": "^3.0.0",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.9",
    "@nomicfoundation/hardhat-toolbox": "^3.0.0",
    "@nomicfoundation/hardhat-verify": "^1.0.0",
    "@nomiclabs/hardhat-ethers": "^2.2.3",
    "@nomiclabs/hardhat-etherscan": "^3.1.7",
    "@typechain/ethers-v6": "^0.4.0",
    "@typechain/hardhat": "^9.0.0",
    "@types/chai": "^4.3.6",
    "@types/mocha": "^10.0.1",
    "@types/node": "^20.5.9",
    "chai": "^4.3.8",
    "ethers": "^6.4.0",
    "hardhat": "^2.17.2",
    "hardhat-gas-reporter": "^1.0.9",
    "solidity-coverage": "^0.8.4",
    "ts-node": "^10.9.1",
    "typechain": "^8.3.1",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@openzeppelin/contracts": "^4.8.2",
    "@phala/ethers": "^6.6.3",
    "@phala/fn": "^0.1.2",
    "@phala/pink-env": "^1.0.10",
    "@phala/sdk": "^0.5.1",
    "dedent": "^1.5.1",
    "dotenv": "^16.0.3"
  }
}
